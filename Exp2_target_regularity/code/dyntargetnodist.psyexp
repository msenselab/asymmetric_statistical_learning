    <Routine name="search">
      <CodeComponent name="search_code">
        <Param name="Before Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="# Coordinates of circle &amp;#10;theta=[0, 45, 90, 135, 180, 225, 270, 315]&amp;#10;Cx=[4*cos(-t*pi/180) for t in theta]&amp;#10;Cy=[4*sin(-t*pi/180) for t in theta]&amp;#10;&amp;#10;# Letter stimuli&amp;#10;dist_letters = ['F', 'L']&amp;#10;item_names = ['dist' + str(i) for i in [0, 1, 2, 3, 4, 5, 6, 7]]&amp;#10;items = [visual.TextStim(win, units='deg',  pos=(Cx[i], Cy[i]), name = item_names[i]) for i in [0, 1, 2, 3, 4, 5, 6, 7]]&amp;#10;&amp;#10;# Target&amp;#10;tar_letters = 'T'&amp;#10;target = visual.TextStim(win, units='deg',  name = 'Target')&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="theta = [0, 45, 90, 135, 180, 225, 270, 315];&amp;#10;Cx = function () {&amp;#10;    var _pj_a = [], _pj_b = theta;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var t = _pj_b[_pj_c];&amp;#10;        _pj_a.push((4/ScreenHeight * Math.cos((((- t) * Math.PI) / 180))));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;Cy = function () {&amp;#10;    var _pj_a = [], _pj_b = theta;&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var t = _pj_b[_pj_c];&amp;#10;        _pj_a.push((4/ScreenHeight * Math.sin((((- t) * Math.PI) / 180))));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;dist_letters = [&quot;F&quot;, &quot;L&quot;];&amp;#10;item_names = function () {&amp;#10;    var _pj_a = [], _pj_b = [0, 1, 2, 3, 4, 5, 6, 7];&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var i = _pj_b[_pj_c];&amp;#10;        _pj_a.push((&quot;dist&quot; + i.toString()));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;items = function () {&amp;#10;    var _pj_a = [], _pj_b = [0, 1, 2, 3, 4, 5, 6, 7];&amp;#10;    for (var _pj_c = 0, _pj_d = _pj_b.length; (_pj_c &lt; _pj_d); _pj_c += 1) {&amp;#10;        var i = _pj_b[_pj_c];&amp;#10;        _pj_a.push(new visual.TextStim({&quot;win&quot;: psychoJS.window, &quot;units&quot;: &quot;height&quot;, &quot;height&quot;: 0.8/ScreenHeight, &quot;pos&quot;: [Cx[i], Cy[i]], &quot;name&quot;: item_names[i]}));&amp;#10;    }&amp;#10;    return _pj_a;&amp;#10;}&amp;#10;.call(this);&amp;#10;tar_letters = &quot;T&quot;;&amp;#10;target = new visual.TextStim({&quot;win&quot;: psychoJS.window, &quot;units&quot;: &quot;height&quot;, &quot;height&quot;: 0.8/ScreenHeight, &quot;name&quot;: &quot;Target&quot;});&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="Distractors = [1, 1, 1, 1, 1, 1, 1, 1];&amp;#10;DistLetters = [Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random()), Math.round(Math.random())] &amp;#10;TarCoords = [(4/ScreenHeight * Math.cos((((- theta[tar_pos]) * Math.PI) / 180))), (4/ScreenHeight * Math.sin((((- theta[tar_pos]) * Math.PI) / 180)))];&amp;#10;Distractors[tar_pos] = 0;&amp;#10;if ((tar_ori === 1)) {&amp;#10;    corr_ans = &quot;f&quot;;&amp;#10;} else {&amp;#10;    corr_ans = &quot;j&quot;;&amp;#10;}&amp;#10;for (var i = 0, _pj_a = theta.length; (i &lt; _pj_a); i += 1) {&amp;#10;    if ((Distractors[i] === 1)) {&amp;#10;        items[i].setText(dist_letters[DistLetters[i]]);&amp;#10;        items[i].setOri((90 + (180 * Math.round(Math.random()))));&amp;#10;        items[i].setAutoDraw(true);&amp;#10;    } else {&amp;#10;        items[i].setAutoDraw(false);&amp;#10;    }&amp;#10;}&amp;#10;target.setText(tar_letters);&amp;#10;if ((tar_ori === 1)) {&amp;#10;    target.setOri(270);&amp;#10;} else {&amp;#10;    target.setOri(90);&amp;#10;}&amp;#10;target.setPos(TarCoords);&amp;#10;target.setAutoDraw(true);&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="# Reset active nonsingletons&amp;#10;Distractors=[1, 1, 1, 1, 1, 1, 1, 1]&amp;#10;DistLetters=np.random.randint(0,2,8)&amp;#10;&amp;#10;# Target coordinates&amp;#10;TarCoords=[4*cos(-theta[tar_pos]*pi/180), 4*sin(-theta[tar_pos]*pi/180)]&amp;#10;&amp;#10;# Remove distractor from target position&amp;#10;Distractors[tar_pos]=0&amp;#10;&amp;#10;# Define correct answer&amp;#10;if (tar_ori==1):&amp;#10;    corr_ans='f'&amp;#10;else:&amp;#10;    corr_ans='j'&amp;#10;&amp;#10;# Distractors&amp;#10;for i in range(0, len(theta)):&amp;#10;    if Distractors[i]==1:&amp;#10;        items[i].setText(dist_letters[DistLetters[i]])&amp;#10;        items[i].setOri(90+180*np.random.randint(0,2))&amp;#10;        items[i].setAutoDraw(True)&amp;#10;    else:&amp;#10;        items[i].setAutoDraw(False)&amp;#10;&amp;#10;# Target &amp;#10;target.setText(tar_letters)&amp;#10;if tar_ori==1:&amp;#10;    target.setOri(90)&amp;#10;else:&amp;#10;    target.setOri(270)&amp;#10;target.setPos(TarCoords)&amp;#10;target.setAutoDraw(True)" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Both" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="for (var i = 0, _pj_a = theta.length; (i &lt; _pj_a); i += 1) {&amp;#10;    items[i].setAutoDraw(false);&amp;#10;}&amp;#10;target.setAutoDraw(false);&amp;#10;" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="# Middle circle&amp;#10;for i in range(0, len(theta)):&amp;#10;   items[i].setAutoDraw(False)&amp;#10;&amp;#10;# Target &amp;#10;target.setAutoDraw(False)&amp;#10;" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="search_code" valType="code"/>
