        <Param name="Begin JS Experiment" updates="constant" val="block_trials = 84;&amp;#10;num_block = 10;&amp;#10;N_trials = (block_trials * num_block);&amp;#10;breaktext = new visual.TextStim({&quot;win&quot;: psychoJS.window, &quot;text&quot;: &quot;Break&quot;, &quot;units&quot;: &quot;height&quot;, &quot;height&quot;: 0.04});&amp;#10;" valType="extendedCode"/>
